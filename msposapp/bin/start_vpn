#!/bin/bash

[ "$1" != "" ] && CONTAINER="$1" && docker exec $CONTAINER systemctl start ipsec && exit 0

# check for running container
if [ -f ./.container ]; then
  CONTAINER="`cat .container`"
else
  echo "No Running Container!"
  exit 1
fi

docker exec $CONTAINER systemctl start ipsec

#wget -qO- http://ipv4.icanhazip.com; echo
